{
  "name": "@dependency-health-monitor/frontend",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "npx @azure/static-web-apps-cli start .output/public --api-location .output/server -p 5050",
    "lint": "npm run test:lint -- --fix",
    "test:lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path ../../.gitignore",
    "test:unit": "vitest run --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "devDependencies": {
    "nuxt": "3.0.0-rc.4",
    "sass": "^1.53.0"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.1.9",
    "pinia": "^2.0.14",
    "primeflex": "^3.1.3",
    "primevue": "^3.12.5"
  }
}
