{
  "platform": {
    "apiRuntime": "node:16"
  },
  "routes": [
    {
      "route": "/404",
      "rewrite": "/404.html"
    },
    {
      "route": "/200",
      "rewrite": "/200.html"
    },
    {
      "route": "/",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/api/server"
  }
}