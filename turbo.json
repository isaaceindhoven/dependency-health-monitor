{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
      "build": {
          "dependsOn": ["^build", "lint"],
          "outputs": [
              "dist/**",
              "build/**"
          ]
      },
      "test:e2e:ci": {
          "dependsOn": ["build"],
          "inputs": ["**/src/**/*.ts", "**/src/**/*.vue", "**/cypress/**/*.ts", "**/cypress/**/*.json"],
          "outputs": []
      },
      "test:unit": {
          "dependsOn": ["lint"],
          "inputs": ["**/src/**/*.ts"],
          "outputs": []
      },
      "dev": {
          "cache": false
      },
      "lint": {
          "outputs": []
      }
  }
}
