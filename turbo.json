{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "lint"],
      "outputs": ["dist/**", "build/**"]
    },
    "test:e2e:ci": {
      "dependsOn": ["^build"],
      "inputs": ["**/src/**/*.ts", "**/src/**/*.vue", "**/cypress/**/*.ts", "**/cypress/**/*.json"],
      "outputs": []
    },
    "test:unit": {
      "dependsOn": ["^build"],
      "inputs": ["**/src/**/*.ts"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "preview": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "test:lint": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
